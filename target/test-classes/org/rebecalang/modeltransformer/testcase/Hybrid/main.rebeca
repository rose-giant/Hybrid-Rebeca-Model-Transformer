physicalclass Car(1) {
    knownrebecs {
    }

    statevars {
        @Real float t;
    }

    Car() {
        delay(1.0, 2.0);
        int r;
        r = t + 1;
        if (r < 1) {
            r = 3 * r;
        }
    }

    mode Rolling {
        inv(t <= 2) {
            t' = 2 + 3.1;
            t'' = 3*t';
        }

        guard(t == 1) {
            t = 6.6*t + 1;
            setMode(none);
        }
    }

}

